<Window x:Class="skelet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:skelet"
        mc:Ignorable="d"
        Title="skelet" SizeToContent="WidthAndHeight" >
    <StackPanel>
        <Label Content="state : " Name="lbl"></Label>
        <StackPanel Orientation="Horizontal">
            <Label Content="phone : "></Label>
            <TextBox  Width="200" Name="txt_phone" KeyDown="txt_phone_KeyDown"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="password : "></Label>
            <TextBox  Width="200"  Name="txt_pass" KeyDown="txt_pass_KeyDown"></TextBox>
        </StackPanel>
    </StackPanel>
</Window>
